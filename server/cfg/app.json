{
  "auth": {
    "appKey":                  "ag",
    "oauth": {
      "github":                "{{undefine}}",
      "google": {
        "short":               "gp",
        "login":               true,
        "signup":              true,
        "clientID":            "{{required}}",
        "clientSecret":        "{{required}}",
        "scope":               ["profile","email",
                                "https://www.googleapis.com/auth/userinfo.email",
                                "https://www.googleapis.com/auth/userinfo.profile",
                                "https://mail.google.com/gmail.readonly"]
      }
    }
  },
  "comm": {
    "mode":                    "stub",
    "transports":              ["ses"]
  },
  "http": {
    "port":                    3436,
    "static": {
      "dirs":                  "htp/static",
      "cdn": {
        "atchd":               "ag",
        "post":                "cms/post",
        "ui":                  "ui/v1"
      },
      "favicon": {
        "root":                "htp/static",
        "headers":             { "Content-Type":"image/x-icon" }
      },
      "host":                  "https://cdn.37paulst.com",
      "sitemap": {
        "file":                "htp/static/sitemap.xml",
        "host":                "https://www.37paulst.com"
      }
    }
  },
  "log": {
    "appKey":                  "ag",
    "analytics": {
      "on":                    false,
      "gtag":                  false,
      "model": {
        "collections":         { "issue": "Issue" },
        "mongoUrl":            "mongodb://localhost/argu",
        "enums":               false
      }
    },
    "comm":                    true,
    "errors": {
      "mail":                  { "to": "dev@argu.src", "sender": "ERR <err@argu.src>" },
      "ignore":                "Failed to decode param|authorization code has"
    }
  },
  "logic": {
  },
  "middleware": {
    "ctx": {                   "ud": { "ban": "AhrefsBot|Baiduspider|" } },
    "dev":                     { "off": true },
    "session": {
      "authdData":             "_id name",
      "secret":                "arguing",
      "store":                 { "collection": "sessions" }
    }
  },
  "model": {
    "cache": {
      "on":                    true,
      "require":               "templates:cached,tags:cached,laws:cached,contents:cached"
    },
    "domain": {
      "mongodb":               "mongodb://localhost/argu"
    }
  },
  "routes": {
    "bot":                     { "off": false },
    "reroute":                 { "off": true },
    "adm":                     { "type": "html", "mount": "/adm", "layout": "adm" },
    "set":                     { "off": true, "type": "html", "mount": "/sets", "layout": "argu" },
    "print":                   { "off": true, "type": "html", "mount": "/print" },
    "page":                    { "type": "html" }
  },
  "templates": {
    "dirs": {
      "helpers":               "pl8/fn",
      "partials":              "pl8/ui/part",
      "views":                 "pl8/ui"
    }
  },
  "wrappers": {
    "dirs":                    "wrp",
    "ses": {
      "accessKeyId":           "{{required}}",
      "secretAccessKey":       "{{required}}",
      "region":                "us-west-2"
    },
    "gmail": {
      "accessToken":          "{{required}}",
      "refreshToken":         "{{required}}"
    }
  }
}
